<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>SCRH @(ViewBag.Title != null ? "|" : null) @ViewBag.Title</title>
    <link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons" />
    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css">
    <link href="//cdn.jsdelivr.net/npm/select2@4.1.0-beta.1/dist/css/select2.min.css" rel="stylesheet" />
    @Styles.Render("~/Content/layout-interno")
    @Scripts.Render("~/bundles/modernizr")
    @RenderSection("styles", required: false)
    @Styles.Render("~/Content/lobibox")
    @Styles.Render("~/Content/sweetalert2")
</head>
<body>
    <div class="wrapper">
        <div class="sidebar" data-color="azure" data-background-color="white" data-image="#">
            @RenderSection("Menu", true)
        </div>
        <div class="main-panel">
            @Html.Partial("_MenuTopoInterno")
            <div class="content">
                <div class="container-fluid">
                    <div class="row">
                        @RenderBody()
                    </div>
                </div>
            </div>
        </div>
    </div>

    <script src="//code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
    @*@Scripts.Render("~/bundles/jquery")*@
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/layout-interno")
    @Scripts.Render("~/bundles/lobibox")
    @Scripts.Render("~/bundles/sweetalert2")
    @Scripts.Render("~/bundles/mask")
    @Scripts.Render("~/bundles/loading")

    @RenderSection("scripts", required: false)
    @Html.Partial("_Toastr")


    <script>
        $(document).ready(function () {
            $(document).on("click", "button[type = 'submit']", function () {
                $("body").LoadingOverlay("show");
            });
            $(document).on("click", "a.nav-link", function () {
                $("body").LoadingOverlay("show");
            });
            $(document).on("click", ".btn-abrirModal", function () {
                $("body").LoadingOverlay("show");
            });
            $(document).on("click", ".dropdown-item", function () {
                $("body").LoadingOverlay("show");
            });
        });
    </script>
</body>
</html>
